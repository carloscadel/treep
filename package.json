    "node": "8.9.4"
  },
  "dependencies": {
    "@babel/runtime": "^7.3.4",
    "@types/node": "^11.10.4",
    "babel-polyfill": "^6.26.0",
    "braces": "^2.3.2",
    "lodash": "^4.17.11",
    "minimatch": "^3.0.4",
    "yarn": "^1.13.0"
  },
  "devDependencies": {}
}
